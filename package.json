{
  "name": "barhandles",
  "version": "0.5.3",
  "description": "Extract references from Handlebars templates",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wspringer/barhandles/"
  },
  "scripts": {
    "build": "coffee -m -o lib -c src",
    "prepublish": "npm run build",
    "test": "mocha --recursive -C --require coffee-script/register test/**/*.coffee"
  },
  "keywords": [
    "handlebars"
  ],
  "author": "Wilfred Springer",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.0.0",
    "coffee-script": "^1.9.3",
    "mocha": "^6.1.4",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "handlebars": ">=3.0.3",
    "lodash": "^4.17.11"
  }
}
