{
    "name": "barhandles",
    "version": "0.6.1",
    "description": "Extract references from Handlebars templates",
    "main": "lib/index.js",
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bborn/barhandles/"
    },
    "scripts": {
        "build": "tsc -p .",
        "prepublish": "npm run build",
        "test": "jest"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "keywords": [
        "handlebars"
    ],
    "author": "Wilfred Springer",
    "license": "ISC",
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "handlebars": "^4.7.6",
        "lodash": "^4.17.15"
    }
}
